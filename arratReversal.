#include <stdio.h>
#include <stdlib.h>

int main(){
int num, *arr, i;
    scanf("%d", &num);
    
    arr = (int*) malloc(num * sizeof(int));
    if (arr == NULL)
     return 1; 

    
    for(i = 0; i < num; i++) {
        scanf("%d", &arr[i]);
    }
int start = 0;
int end = num - 1;
while (start < end) {
    // Swap arr[start] and arr[end]
    int temp = arr[start];
    arr[start] = arr[end];
    arr[end] = temp;
    
    start++; // Move inwards from the start
    end--;   // Move inwards from the end
}


for(i = 0; i < num; i++) {
    printf("%d ", arr[i]);
}


free(arr); 
return 0;

    return 0;
}
